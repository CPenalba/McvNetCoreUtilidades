@model List<Coche>

<h1>Coches con AJAX</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                IdCoche
            </th>
            <th>
               Marca
            </th>
            <th>
               Modelo
            </th>
            <th>
                Imagen
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCoche)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo)
            </td>
            <td>
                <img src="@item.Imagen" style="width:150px; height:150px"/>
            </td>
        </tr>
}
    </tbody>
</table>
